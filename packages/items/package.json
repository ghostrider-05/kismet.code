{
  "name": "@kismet.ts/items",
  "version": "0.1.3",
  "description": "A package containing default items for kismet",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "files": [
    "./dist/src/"
  ],
  "type": "module",
  "scripts": {
    "prestart": "tsc --project ./tsconfig.json",
    "prestart:script": "tsc --project ./tsconfig-scripts.json",
    "create-tree": "pnpm prestart && node --experimental-json-modules ./dist/scripts/tree.js",
    "create-tree2": "pnpm prestart:script && node --experimental-json-modules ./dist/scripts/tree2.js",
    "create-trees": "pnpm create-tree && pnpm create-tree2",
    "update": "pnpm prestart && node --experimental-json-modules ./dist/scripts/update.js",
    "upload": "pnpm prestart && node --experimental-json-modules ./dist/scripts/upload.js",
    "update:rl": "extract.bat && pnpm prestart:script && pnpm update && pnpm create-trees && pnpm upload"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ghostrider-05/kismet.ts.git"
  },
  "author": "ghostrider-05",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ghostrider-05/kismet.ts/issues"
  },
  "homepage": "https://github.com/ghostrider-05/kismet.ts#readme",
  "devDependencies": {
    "@kismet.ts/parsers-node": "workspace:^",
    "@types/inquirer": "^9.0.5",
    "inquirer": "^9.2.11",
    "node-fetch": "^3.3.2"
  },
  "dependencies": {
    "@kismet.ts/core": "workspace:^",
    "@kismet.ts/shared": "workspace:^"
  },
  "typedoc": {
    "entryPoint": "./src/index.ts",
    "readmeFile": "README.md",
    "displayName": "@kismet.ts/items",
    "tsconfig": "./tsconfig.json"
  }
}
